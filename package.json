{
  "name": "tree-house",
  "version": "2.1.1",
  "description": "NodeJS project using ExpressJS to create an easy, secure and customisable API layer",
  "main": "./build/index.js",
  "scripts": {
    "clean": "rm -rf build",
    "build": "npm run clean && node_modules/.bin/babel src -d build",
    "start": "npm run build && node build/index.js",
    "prepublish": "npm run build",
    "lint": "node_modules/.bin/eslint ./ --cache --config ./.eslintrc --ignore-pattern .gitignore",
    "test": "jest --forceExit",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.js --forceExit"
  },
  "precommit": [
    "lint"
  ],
  "dependencies": {
    "body-parser": "~1.18.2",
    "bunyan": "^1.8.10",
    "cluster": "^0.7.7",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-rate-limit": "^2.9.0",
    "helmet": "^3.9.0",
    "http": "0.0.0",
    "https": "^1.0.0",
    "js-yaml": "^3.10.0",
    "lodash": "^4.17.4",
    "os": "^0.1.1",
    "swagger-ui-express": "^2.0.9"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.1.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-node": "^5.1.1",
    "jest": "^21.2.1",
    "mock-express-request": "^0.2.0",
    "mock-express-response": "^0.2.1",
    "pre-commit": "^1.2.2",
    "supertest": "^3.0.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knor-el-snor/tree-house.git"
  },
  "keywords": [
    "NodeJS",
    "ExpressJS",
    "Treehouse",
    "API",
    "Secure",
    "Customisable"
  ],
  "author": "Brent Van Geertruy <brentvangeertruy@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/knor-el-snor/tree-house/issues"
  },
  "homepage": "https://github.com/knor-el-snor/tree-house#readme",
  "directories": {
    "test": "test"
  }
}
