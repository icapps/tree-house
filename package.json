{
  "name": "tree-house",
  "version": "3.3.2",
  "description": "NodeJS utilities and handy helpers extending ExpressJS functionalities",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./build/*",
    "build": "npm run clean && tsc",
    "start": "npm run build && node build/index.js",
    "lint": "tslint 'src/**/*.ts'",
    "test": "jest --forceExit",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.ts --forceExit",
    "test:watch": "jest --watchAll",
    "pretest": "npm run build",
    "prepare": "npm run build",
    "release": "node_modules/np/cli.js --no-cleanup"
  },
  "precommit": [
    "lint"
  ],
  "dependencies": {
    "@types/node": "^10.12.18",
    "body-parser": "~1.18.3",
    "cors": "~2.8.5",
    "express": "~4.16.4",
    "express-brute": "~1.0.1",
    "express-brute-redis": "~0.0.1",
    "express-validation": "~1.0.2",
    "helmet": "~3.14.0",
    "https": "~1.0.0",
    "js-yaml": "~3.12.0",
    "swagger-ui-express": "~4.0.1"
  },
  "devDependencies": {
    "@types/cors": "~2.8.4",
    "@types/express": "~4.16.0",
    "@types/express-brute": "~0.0.37",
    "@types/express-brute-redis": "~0.0.1",
    "@types/helmet": "~0.0.42",
    "@types/jest": "~23.3.9",
    "@types/joi": "~14.0.0",
    "@types/supertest": "~2.0.6",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "joi": "~14.0.4",
    "np": "^3.0.4",
    "pre-commit": "^1.2.2",
    "redis-mock": "0.37.0",
    "supertest": "^3.3.0",
    "ts-jest": "~22.4.6",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icapps/tree-house.git"
  },
  "keywords": [
    "NodeJS",
    "Treehouse",
    "API",
    "Secure",
    "Customisable"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/icapps/tree-house/issues"
  },
  "homepage": "https://github.com/icapps/tree-house#readme",
  "directories": {
    "test": "tests"
  }
}
