{
  "name": "tree-house",
  "version": "2.2.3",
  "description": "NodeJS project using ExpressJS to create an easy, secure and customisable API layer",
  "main": "./build/index.js",
  "scripts": {
    "clean": "rm -rf ./build/*",
    "build": "yarn clean && tsc",
    "start": "yarn build && node build/index.js",
    "lint": "tslint 'src/**/*.ts'",
    "test": "jest --forceExit",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.ts --forceExit",
    "test:watch": "jest --watchAll",
    "pretest": "yarn build",
    "prepublish": "yarn build"
  },
  "precommit": [
    "lint"
  ],
  "dependencies": {
    "body-parser": "~1.18.2",
    "cluster": "~0.7.7",
    "cors": "~2.8.4",
    "express": "~4.16.2",
    "express-rate-limit": "~2.11.0",
    "helmet": "~3.12.0",
    "https": "~1.0.0",
    "js-yaml": "~3.11.0",
    "lodash": "~4.17.4",
    "os": "~0.1.1",
    "swagger-ui-express": "~2.0.9"
  },
  "devDependencies": {
    "@types/jest": "~22.2.0",
    "coveralls": "^3.0.0",
    "mock-express-request": "^0.2.0",
    "mock-express-response": "^0.2.1",
    "pre-commit": "^1.2.2",
    "jest": "^22.4.2",
    "ts-jest": "~22.4.1",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.7.0",
    "typescript": "^2.7.2",
    "supertest": "^3.0.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icapps/tree-house.git"
  },
  "keywords": [
    "NodeJS",
    "Treehouse",
    "API",
    "Secure",
    "Customisable"
  ],
  "author": "Brent Van Geertruy <brentvangeertruy@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/icapps/tree-house/issues"
  },
  "homepage": "https://github.com/icapps/tree-house#readme",
  "directories": {
    "test": "test"
  }
}
