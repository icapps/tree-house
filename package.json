{
    "name": "tree-house",
    "version": "0.0.3",
    "description": "NodeJS project using ExpressJS to create an easy, secure and customisable API layer",
    "main": "./build/index.js",
    "scripts": {
        "build": "node_modules/.bin/babel src -d build",
        "start": "npm run build && node build/index.js",
        "prepublish": "npm run build",
        "lint": "node_modules/.bin/eslint ./ --cache --config ./.eslintrc --ignore-pattern .gitignore",
        "test": "node_modules/.bin/mocha --reporter spec",
        "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec test/*"
    },
    "precommit": [
        "lint"
    ],
    "dependencies": {
        "bcrypt-nodejs": "0.0.3",
        "body-parser": "~1.16.0",
        "express": "~4.14.1",
        "jsonwebtoken": "^7.4.0",
        "lodash": "^4.17.4",
        "passport": "^0.3.2",
        "passport-jwt": "^2.2.1",
        "passport-local": "^1.0.0"
    },
    "devDependencies": {
        "babel-cli": "^6.22.2",
        "babel-eslint": "^7.1.1",
        "babel-preset-es2015": "^6.24.1",
        "chai": "^3.5.0",
        "chai-things": "^0.2.0",
        "eslint": "^3.15.0",
        "eslint-config-airbnb-base": "^11.0.1",
        "eslint-plugin-import": "^2.2.0",
        "eslint-plugin-mocha": "^4.8.0",
        "eslint-plugin-node": "^3.0.5",
        "mocha": "^3.3.0",
        "pre-commit": "^1.2.2",
        "coveralls": "^2.13.1",
        "istanbul": "^0.4.5"
    },
    "engines": {
        "node": ">=4.2.4"
    },

    "repository": {
        "type": "git",
        "url": "git+https://github.com/knor-el-snor/tree-house.git"
    },
    "keywords": [
        "NodeJS",
        "ExpressJS",
        "Treehouse",
        "API",
        "Secure",
        "Customisable"
    ],
    "author": "Brent Van Geertruy <brentvangeertruy@outlook.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/knor-el-snor/tree-house/issues"
    },
    "homepage": "https://github.com/knor-el-snor/tree-house#readme",
    "directories": {
        "test": "test"
    }
}